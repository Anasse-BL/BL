{
  "name": "Competiwatch",
  "productName": "Competiwatch",
  "description": "A desktop app to track and visualize your competitive match history in Overwatch.",
  "version": "0.1.8",
  "private": true,
  "dependencies": {
    "electron-log": "^2.2.16",
    "nedb": "^1.8.0"
  },
  "bugs": {
    "url": "https://github.com/cheshire137/competiwatch-desktop/issues"
  },
  "homepage": "./",
  "license": "MIT",
  "main": "src/electron-starter.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cheshire137/competiwatch-desktop"
  },
  "author": {
    "name": "Sarah Vessels",
    "email": "cheshire137@gmail.com",
    "url": "https://github.com/cheshire137"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "nf start",
    "electron": "electron .",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "electron-build-macos": "npm run build && node_modules/.bin/electron-packager . \"Competiwatch\" --out=dist --overwrite --prune=true --platform=darwin --arch=x64 --ignore=setupTests.js --ignore=__mocks__ --ignore=__tests__ --ignore=icon-files --ignore=TestHelpers.js --ignore=win-installer-config.json --ignore=screenshots --icon=icon-files/mac/icon.icns",
    "electron-build-windows": "npm run build && node node_modules\\electron-packager\\cli.js . --out=dist --overwrite --asar=true --icon=icon-files/win/icon.ico --prune=true --version-string.CompanyName=SV --version-string.FileDescription=SV --version-string.ProductName=Competiwatch --platform=win32 --arch=x64 --ignore=setupTests.js --ignore=__mocks__ --ignore=win-installer-config.json --ignore=__tests__ --ignore=TestHelpers.js --ignore=icon-files --ignore=screenshots",
    "electron-build-windows-installer": "node_modules\\.bin\\electron-installer-windows --src dist\\Competiwatch-win32-x64 --config win-installer-config.json"
  },
  "devDependencies": {
    "chart.js": "^2.7.2",
    "csv-parse": "^2.5.0",
    "csv-stringify": "^3.1.1",
    "electron": "^2.0.8",
    "electron-packager": "^12.1.0",
    "electron-winstaller": "^2.6.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "foreman": "^3.0.1",
    "ionicons": "^4.2.4",
    "jest-enzyme": "^6.0.2",
    "primer": "^10.7.0",
    "react": "^16.4.1",
    "react-autocomplete": "^1.8.1",
    "react-chartjs-2": "^2.7.4",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "react-scroll-to-component": "^1.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!node_modules/**",
      "!src/lib/**",
      "!dist/**",
      "!build/**",
      "!coverage/**"
    ]
  }
}
