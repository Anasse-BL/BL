{
  "name": "Competiwatch",
  "description": "A desktop app to track and visualize your competitive match history in Overwatch.",
  "version": "0.1.3",
  "private": true,
  "dependencies": {
    "electron-log": "^2.2.16",
    "nedb": "^1.8.0"
  },
  "bugs": {
    "url": "https://github.com/cheshire137/competiwatch-desktop/issues"
  },
  "homepage": "./",
  "license": "MIT",
  "main": "src/electron-starter.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cheshire137/competiwatch-desktop"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "nf start",
    "electron": "electron .",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "electron-build-macos": "if [ -d \"dist/Competiwatch-darwin-x64\" ]; then rm -r \"dist/Competiwatch-darwin-x64\"; fi && npm run build && node_modules/electron-packager/cli.js . \"Competiwatch\" --out=dist --platform=darwin --arch=all",
    "electron-build-windows": "npm run build && node node_modules\\electron-packager\\cli.js . --out=dist --overwrite --platform=win32 --arch=all"
  },
  "devDependencies": {
    "chart.js": "^2.7.2",
    "csv-parse": "^2.5.0",
    "csv-stringify": "^3.1.1",
    "electron": "^2.0.4",
    "electron-packager": "^12.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "foreman": "^3.0.1",
    "ionicons": "^4.2.4",
    "jest-enzyme": "^6.0.2",
    "primer": "^10.7.0",
    "react": "^16.4.1",
    "react-autocomplete": "^1.8.1",
    "react-chartjs-2": "^2.7.4",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "react-scroll-to-component": "^1.0.2"
  }
}
