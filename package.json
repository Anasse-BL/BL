{
  "name": "Competiwatch",
  "description": "A desktop app to track and visualize your competitive match history in Overwatch.",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "csv-generate": "^2.0.2",
    "csv-parse": "^2.5.0",
    "csv-stringify": "^3.1.1",
    "electron-log": "^2.2.16",
    "nedb": "^1.8.0",
    "react": "^16.4.1",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "bugs": {
    "url": "https://github.com/cheshire137/competiwatch-desktop/issues"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "nf start",
    "electron": "electron .",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "electron-build-macos": "if [ -d \"dist/Competiwatch-darwin-x64\" ]; then rm -r \"dist/Competiwatch-darwin-x64\"; fi && npm run build && node_modules/electron-packager/cli.js . \"Competiwatch\" --out=dist --ignore=node_modules --platform=darwin --arch=all",
    "electron-build-windows": "npm run build && node node_modules\\electron-packager\\cli.js . --out=dist --overwrite --ignore=node_modules --platform=win32 --arch=all"
  },
  "devDependencies": {
    "electron": "^2.0.4",
    "electron-packager": "^12.1.0",
    "foreman": "^3.0.1",
    "ionicons": "^4.2.4",
    "primer": "^10.7.0"
  }
}
